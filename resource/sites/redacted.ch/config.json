      "name": "Applications",
      "enabled": false
    },
    {
      "queryString": "filter_cat[3]=1",
      "name": "E-Books",
      "enabled": false
    },
    {
      "queryString": "filter_cat[4]=1",
      "name": "Audiobooks",
      "enabled": false
    },
    {
      "queryString": "filter_cat[5]=1",
      "name": "E-Learning Videos",
      "enabled": false
    },
    {
      "queryString": "filter_cat[6]=1",
      "name": "Comedy",
      "enabled": false
    },
    {
      "queryString": "filter_cat[7]=1",
      "name": "Comics",
      "enabled": false
    }
