    "userExtendInfo": {
      "merge": true,
      "fields": {
        "uploaded": {
          "selector": ["td.rowfollow:contains('分享率')", "td.rowhead:contains('传输') + td", "td.rowhead:contains('傳送') + td", "td.rowhead:contains('Transfers') + td"],
