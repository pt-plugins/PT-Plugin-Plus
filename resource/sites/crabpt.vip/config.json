  "categories": [],
  "searchEntryConfig": {
    "fieldSelector": {
      "progress": {
        "selector": [
          "div.progressBar > div"
        ],
        "filters": [
          "query.attr('style')||''",
          "query.match(/width:.?(\\d.+)%/)",
          "(query && query.length>=2)?query[1]:null"
        ]
      },
      "status": {
        "selector": [
          "div.progressBar.doing > div",
          "div.progressBar > div"
        ],
        "switchFilters": [
          [
            "query.attr('style')||''",
            "query.match(/width:.?(\\d.+)%/)",
            "(query && query.length>=2)?query[1]:0",
            "parseInt(query)==100?2:1"
          ],
          [
            "255"
          ]
        ]
      }
    }
  },
  "plugins": [
    {
      "name": "保种列表",
      "pages": [
        "/rescue.php"
      ],
      "scripts": [
        "/schemas/NexusPHP/common.js",
        "/schemas/NexusPHP/torrents.js"
      ]
    }
  ],
  "mergeSchemaTagSelectors": true,
