{
  "name": "Rousi",
  "description": "你也喜欢Rousi 对吧？",
  "url": "https://rousi.zip/",
  "icon": "https://rousi.zip/favicon.ico",
  "tags": ["影视"],
  "schema": "NexusPHP",
  "host": "rousi.zip",
  "plugins": [
    {
      "name": "特殊插件",
      "pages": ["/torrents.php"],
      "scripts": ["/libs/album/album.js", "torrents.js"],
      "styles": ["/libs/album/style.css"]
    }
  ],
  "searchEntry": [
    {
      "entry": "/torrents.php?search=$key$",
      "name": "全部",
      "resultType": "html",
      "parseScriptFile": "/schemas/NexusPHP/getSearchResult.js",
      "resultSelector": "table.torrent_list:last > tbody > tr"
    }
  ],
  "patterns": {
    "torrentLinks": ["*://*/*"]
  },
  "parser": {
    "downloadURL": "解析脚本内容"
  },
  "torrentTagSelectors": [
    {
      "name": "Free",
      "selector": "img.pro_free",
      "color": "blue"
    }
  ],
  "categories": [
    {
      "entry": "*",
      "result": "cat$id$=1",
      "category": [
        {
          "id": 20,
          "name": "综合"
        }
      ]
    }
  ]
}
