        "selector": [
          ".torrentname td:first-child > div[title]:last-child"
        ],
        "filters": [
          "query ? query.attr('title').split(' ')[0] : null",
          "query === 'leeching' ? 1 : query === 'seeding' ? 2 : query === 'inactivity' ? 255 : null"
        ]
