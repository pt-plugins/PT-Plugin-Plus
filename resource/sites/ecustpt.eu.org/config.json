  "searchEntryConfig": {
    "merge": true,
    "fieldSelector": {
      "progress": {
        "selector": [
          "td.snatched_no_yes, td.snatched_yes_yes",
          "td.snatched_no_no, td.snatched_yes_no",
          ""
        ],
        "switchFilters": [
          [
            "100"
          ],
          [
            "0"
          ],
          [
            "null"
          ]
        ]
      },
      "status": {
        "selector": [
          "td.snatched_no_yes, td.snatched_yes_yes",
          "td.snatched_no_no, td.snatched_yes_no"
        ],
        "switchFilters": [
          [
            "2"
          ],
          [
            "3"
          ]
        ]
      }
    }
  },
  "searchEntry": [
    {
      "name": "全部",
      "enabled": true
    }
  ],
