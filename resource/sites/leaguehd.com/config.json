{
    "name": "LeagueHD",
    "timezoneOffset": "+0800",
    "schema": "NexusPHP",
    "url": "https://leaguehd.com/",
    "description": "LeagueHD",
    "icon": "https://leaguehd.com/favicon.ico",
    "tags": [
      "影视",
      "综合"
    ],
    "host": "leaguehd.com",
    "collaborator": "enigmaz",
    "searchEntryConfig": {
      "page": "/torrents_movie.php",
      "queryString": "search=$key$",
      "parseScriptFile": "getSearchResult.js",
      "area": [{
        "name": "标题",
        "appendQueryString": "&search_area=name"
      }],
      "fieldSelector": {
        "progress": {
          "selector": ["> td:eq(8)"],
          "filters": ["query.text()==='--'?null:parseFloat(query.text())"]
        },
        "status": {
          "selector": ["> td:eq(8)"],
          "filters": ["query.text()==='--'?null:query.is('.peer-active')?(parseFloat(query.text())==100?2:1):(parseFloat(query.text())==100?255:3)"]
        }
      }
    },
    "searchEntry": [{
      "name": "电影",
      "enabled": true
    },{
      "entry": "/torrents_tv.php",
      "name": "电视",
      "enabled": true
    },{
      "entry": "/torrents_music.php",
      "name": "音乐",
      "enabled": true
    },{
      "entry": "/torrents_animate.php",
      "name": "动漫",
      "enabled": true
    },{
      "entry": "/torrents_mv.php",
      "name": "MV",
      "enabled": true
    },{
      "entry": "/torrents_doc.php",
      "name": "纪录片",
      "enabled": true
    },{
      "entry": "torrents_other.php",
      "name": "其他",
      "enabled": true
    }]
  }