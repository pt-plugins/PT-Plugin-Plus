      "requestData": {
        "pageNumber": 1,
        "pageSize": 100,
        "mode": "normal",
        "keyword": "$key$"
      },
      "area": [{
        "name": "IMDB",
        "keyAutoMatch": "^(tt\\d+)$",
        "requestData": {
          "pageNumber": 1,
          "pageSize": 100,
          "imdb": "$key$"
        }
