        "switchFilters": [
          ["query.parent().parent().next().attr('style').split(';')[0].replace('width: ','').replace('px','')/3"],
          ["100"],
          ["null"]
        ]
      },
      "status": {
        "selector": ["img[src='/static/pic/newpic/2s_dl.gif']", "img[src='/static/pic/newpic/2s_up.gif']", "img[src='/static/pic/newpic/2s_dled.gif']"],
        "switchFilters": [
          ["1"],
          ["2"],
          ["255"]
        ]
      }
    }
  },
